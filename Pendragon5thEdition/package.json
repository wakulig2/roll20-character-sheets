{
  "name": "pendragon5thedition",
  "version": "1.0.0",
  "description": "Pendragon compilers",
  "dependencies": {
    "pug": "^2.0.4",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel compile:sass compile:pug",
